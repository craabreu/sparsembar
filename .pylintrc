[MAIN]

ignore=_version.py

[DESIGN]

max-locals=25  # Maximum number of local variables for function/method body
max-attributes=10 # Maximum number of instance attributes for class

[MESSAGES CONTROL]

disable = unexpected-keyword-arg,  # Avoids false-positives with Context.getState method
          too-many-arguments

[FORMAT]

good-names=i,j,k,_,x,y,z
